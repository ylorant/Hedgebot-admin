{
  "name": "hedgebot/hedgebot-web-admin",
  "description": "WebAdmin to chat around at the speed of sound",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "rc",
  "prefer-stable": true,
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "composer/composer": "^2.0.9",
    "symfony/flex": "^1.12.2",
    "symfony/framework-bundle": "^4.4.19",
    "symfony/dotenv": "^4.4.19",
    "symfony/console": "^4.4.19",
    "symfony/yaml": "^4.4.19",
    "symfony/apache-pack": "^1.0.1",
    "symfony/form": "^4.4.19",
    "symfony/security-bundle": "^4.4.19",
    "symfony/translation": "^4.4.19",
    "symfony/swiftmailer-bundle": "^3.5.2",
    "symfony/monolog-bundle": "^3.6.0",
    "symfony/polyfill-apcu": "^1.22.1",
    "twig/twig": "^2.14.3",
    "symfony/asset": "^4.4.19",
    "doctrine/orm": "^2.7.5",
    "doctrine/doctrine-bundle": "^1.12.13",
    "sensio/framework-extra-bundle": "^v4.0.0",
    "friendsofsymfony/jsrouting-bundle": "^2.7.0",
    "mhujer/breadcrumbs-bundle": "^1.5.5",
    "nicklaw5/twitch-api-php": "^1.1",
    "ramsey/uuid": "^3.7",
    "jublonet/codebird-php": "^3.1",
    "knplabs/knp-markdown-bundle": "^1.8",
    "ylorant/hedgebot-api-client": "^1.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.4.19",
    "symfony/profiler-pack": "^v1.0.5",
    "squizlabs/php_codesniffer": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@php bin/console app:setup",
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@php bin/console app:setup",
      "@auto-scripts"
    ]
  }
}
