{
    "name": "hedgebot/hedgebot-web-admin",
    "description": "WebAdmin to chat around at the speed of sound",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "rc",
    "prefer-stable": true,
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "ext-xml": "*",
        "ext-pdo_sqlite": "*",
        "ext-curl": "*",
        "ext-intl": "*",
        "composer/composer": "^2.1",
        "symfony/flex": "^1.13",
        "symfony/framework-bundle": "^5.3",
        "symfony/dotenv": "^5.3",
        "symfony/console": "^5.3",
        "symfony/yaml": "^5.3",
        "symfony/apache-pack": "^1.0.1",
        "symfony/form": "^5.3",
        "symfony/security-bundle": "^5.3",
        "symfony/translation": "^5.3",
        "symfony/swiftmailer-bundle": "^3.5.2",
        "symfony/monolog-bundle": "^3.7",
        "symfony/polyfill-apcu": "^1.23",
        "symfony/intl": "^5.3",
        "twig/twig": "^3.3",
        "twig/extra-bundle": "^3.3",
        "twig/intl-extra": "^3.3",
        "symfony/asset": "^5.3",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.9",
        "sensio/framework-extra-bundle": "^v5.6",
        "friendsofsymfony/jsrouting-bundle": "^2.7",
        "mhujer/breadcrumbs-bundle": "^1.5",
        "nicklaw5/twitch-api-php": "^1.2",
        "ramsey/uuid": "^3.9",
        "jublonet/codebird-php": "^3.1",
        "knplabs/knp-markdown-bundle": "^1.9",
        "ylorant/hedgebot-api-client": "^1.0",
        "symfony/maker-bundle": "^1.33",
        "composer/package-versions-deprecated": "1.11",
        "symfony/proxy-manager-bridge": "^5.3",
        "symfony/validator": "^5.3",
        "symfony/security-csrf": "^5.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.3",
        "symfony/profiler-pack": "^v1.0",
        "squizlabs/php_codesniffer": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@php bin/console app:setup",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@php bin/console app:setup",
            "@auto-scripts"
        ]
    },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "5.3.*"
    }
  }
}
